       <service android:name=".background.Subprocess" />




        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/title_activity_register"></activity>
    </application>


</manifest>